function jsonObjToSortedArray(jsonObject){
	
	var sortable = [];
	for (var entry in jsonObject) {
	    sortable.push([entry, jsonObject[entry]]);
	}
	sortable.sort(function(a, b) {
	    return a[1] > b[1] ? 1 : -1;  
	});
	return sortable;
}


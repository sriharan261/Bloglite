function validationNameCheck(newData){
return (/^[a-zA-Z]+$/.test(newData));
} 

function validatationDropDownCheck(dataValue){
return (dataValue===undefined || dataValue == -1);
}

function isStringEmpty(str) {
    return (!str || str.trim().length === 0 );
}
function addErrorMessages(id, mssg){
	$('#'+id).text(mssg).show();
}

function addAllErrorMessages(errMap){
	let arrKeys = Object.keys(errMap);
	arrKeys.forEach((id)=>{
		if(!isStringEmpty(id)){
			addErrorMessages(id, errMap[id]);
		}
	});
}
function clearAllInput(){
	$('input').val('');
}

function hideAlerts(){
	$(errorAlert).hide();
	$(successAlert).hide();
}
function hideErrorMessages(){
	$('span[id^=err]').hide();
	$('div[id^=err]').hide();
	$('div[id^=success]').hide();
	$('h4[id^=err]').hide();
}




